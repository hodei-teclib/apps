  <record id="idea_box.default_form_view_fo_ab5b2fea-3b48-4b35-8934-de68e0335b54" model="ir.ui.view">
    <field name="active" eval="True" />
    <field name="arch" type="xml">
      <form>
        <header>
          <field name="x_studio_stage_id" widget="statusbar" options="{'clickable': '1'}" />
        </header>
        <sheet string="Ideas">
          <field name="x_studio_kanban_state" widget="state_selection" />
          <widget name="web_ribbon" text="Archived" bg_color="text-bg-danger" invisible="x_active" />
          <field name="x_active" invisible="1" />
          <field class="oe_avatar" widget="image" name="x_studio_image" />
          <div class="oe_title">
            <h1>
              <field name="x_name" required="1" placeholder="Name..." />
            </h1>
          </div>
          <group name="studio_group_b3fa9c">
            <group name="studio_group_b3fa9c_left">
              <field name="x_studio_user_id" widget="many2one_avatar_user" />
              <field name="x_studio_tag_ids" widget="many2many_tags" options="{'color_field': 'x_color'}" />
            </group>
            <group name="studio_group_b3fa9c_right" />
          </group>
          <group>
            <field name="x_studio_notes" placeholder="Type down your notes here..." nolabel="1" colspan="2" />
          </group>
        </sheet>
        <div class="oe_chatter" name="oe_chatter">
          <field name="message_follower_ids" />
          <field name="message_ids" />
          <field name="activity_ids" />
        </div>
      </form>
    </field>
    <field name="groups_id" eval="[(6, 0, [])]" />
    <field name="inherit_id" eval="False" />
    <field name="key" eval="False" />
    <field name="mode">primary</field>
    <field name="model">x_ideas</field>
    <field name="name">Default form view for x_ideas</field>
    <field name="priority">16</field>
    <field name="type">form</field>
  </record>
  <record id="idea_box.default_kanban_view__6443570e-31c2-4fd2-b895-6a9c5f202922" model="ir.ui.view">
    <field name="active" eval="True" />
    <field name="arch" type="xml">
      <kanban default_group_by="x_studio_stage_id" default_order="x_studio_priority desc, x_studio_sequence asc, id desc">
        <field name="x_color" />
        <field name="x_studio_user_id" widget="many2one_avatar_user" />
        <progressbar field="x_studio_kanban_state" colors="{&quot;normal&quot;: &quot;200&quot;, &quot;done&quot;: &quot;success&quot;, &quot;blocked&quot;: &quot;danger&quot;}" />
        <templates>
          <t t-name="kanban-box">
            <div t-attf-class="#{!selection_mode ? kanban_color(record.x_color.raw_value) : ''} oe_kanban_global_click">
              <field class="o_kanban_image" name="x_studio_image" widget="image" options="{&quot;zoom&quot;: true, &quot;background&quot;: true, &quot;preventClicks&quot;: false}" />
              <div class="o_kanban_record_details">
                <div class="o_kanban_record_top" name="studio_auto_kanban_top">
                  <div class="o_kanban_record_headings" name="studio_auto_kanban_headings">
                    <field name="x_studio_priority" widget="boolean_favorite" nolabel="1" />
                    <strong class="o_kanban_record_title" name="studio_auto_kanban_title">
                      <field name="x_name" />
                    </strong>
                  </div>
                  <div class="o_dropdown_kanban dropdown">
                    <a role="button" class="dropdown-toggle o-no-caret btn" data-bs-toggle="dropdown" data-display="static" href="#" aria-label="Dropdown Menu" title="Dropdown Menu">
                      <span class="fa fa-ellipsis-v" />
                    </a>
                    <div class="dropdown-menu" role="menu">
                      <a t-if="widget.editable" role="menuitem" type="edit" class="dropdown-item">Edit</a>
                      <a t-if="widget.deletable" role="menuitem" type="delete" class="dropdown-item">Delete</a>
                      <ul class="oe_kanban_colorpicker" data-field="x_color" />
                    </div>
                  </div>
                </div>
                <div class="o_kanban_record_body" name="studio_auto_kanban_body">
                  <field name="x_studio_tag_ids" options="{'color_field': 'x_color'}" />
                </div>
                <div class="o_kanban_record_bottom" name="studio_auto_kanban_bottom">
                  <div class="oe_kanban_bottom_left" name="studio_auto_kanban_bottom_left" />
                  <div class="oe_kanban_bottom_right" name="studio_auto_kanban_bottom_right">
                    <field name="x_studio_kanban_state" widget="state_selection" />
                    <t t-set="unassigned" t-translation="on">Unassigned</t>
                    <img t-att-src="kanban_image('res.users', 'avatar_128', record.x_studio_user_id.raw_value)" t-att-title="record.x_studio_user_id.value || unassigned" t-att-alt="record.x_studio_user_id.value" class="oe_kanban_avatar o_image_24_cover float-right" />
                  </div>
                </div>
              </div>
            </div>
          </t>
        </templates>
      </kanban>
    </field>
    <field name="groups_id" eval="[(6, 0, [])]" />
    <field name="inherit_id" eval="False" />
    <field name="key" eval="False" />
    <field name="mode">primary</field>
    <field name="model">x_ideas</field>
    <field name="name">Default kanban view for x_ideas</field>
    <field name="priority">16</field>
    <field name="type">kanban</field>
  </record>
